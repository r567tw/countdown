{
  "name": "倒數計時器",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "倒數屬於你重要的日子",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "action": {
    "default_title": "倒數計時器",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "options_page": "src/option/option.html",
  "permissions": [
    "storage"
  ]
}